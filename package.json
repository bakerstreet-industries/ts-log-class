  "devDependencies": {
    "@types/chai": "^4.0.6",
    "@types/chai-spies": "0.0.0",
    "@types/mocha": "^2.2.44",
    "chai": "^4.1.2",
    "chai-spies-next": "^0.9.3",
    "coveralls": "^3.0.0",
    "mocha": "^4.0.1",

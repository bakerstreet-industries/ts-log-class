  "bugs": {
    "url": "https://github.com/bakerstreet-industries/ts-logger/issues"
  },
  "homepage": "https://github.com/bakerstreet-industries/ts-logger#readme",
  "devDependencies": {
    "@types/chai": "^4.0.6",
    "@types/chai-spies": "0.0.0",
    "@types/mocha": "^2.2.44",
    "chai": "^4.1.2",
    "chai-spies": "^0.7.1",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "ts-loader": "^3.2.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2",
